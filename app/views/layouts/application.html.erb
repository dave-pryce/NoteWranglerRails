<!DOCTYPE html>
<html lang="en" ng-app="NoteWrangler">
<head>
  <title>Note Wrangler Rails</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>

  <script type="text/javascript" src="javascript/angular.js"></script>
  <script type="text/javascript" src="javascript/vendor/angular-route.js"></script>
  <script type="text/javascript" src="javascript/vendor/angular-resource.js"></script>

</head>
<body>

<!-- custom directive -->
<nw-page-nav></nw-page-nav>

<div ng-view class="container-fluid"> </div>

<div ng-include src="'templates/footer.html'"></div>

</body>




<!-- load md5 crypto library -->
<script src="https://crypto-js.googlecode.com/svn/tags/3.1.2/build/rollups/md5.js"></script>
</html>
